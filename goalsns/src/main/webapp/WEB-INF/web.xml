<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>MemberFrontController</servlet-name>
		<servlet-class>goalsns.controller.MemberFrontController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProfileFrontController</servlet-name>
		<display-name>ProfileFrontController</display-name>
		<description></description>
		<servlet-class>goalsns.controller.ProfileFrontController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>MemberFrontController</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProfileFrontController</servlet-name>
		<url-pattern>*.fo</url-pattern>
	</servlet-mapping>
	
</web-app>
